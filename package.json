{
  "name": "netsuite-react-starter-kit",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@tanstack/react-query": "^5.62.11",
    "cra-template": "1.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^4.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-app-rewired build && npm run clean",
    "clean": "./scripts/clean-up.sh",
    "deploy": "npm run build && suitecloud project:deploy",
    "api-server": "json-server --watch data/db.json --port 8000",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "json-server": "^1.0.0-beta.3",
    "prop-types": "^15.8.1",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.4.17",
    "jest": "29.7.0",
    "@types/jest": "29.5.11",
    "@oracle/suitecloud-unit-testing": "^1.5.0"
  }
}
